{
  "apps": {
    "name": "opencv-nodejs-app",
    "script": "./app.js",
    "exec_mode": "fork",
    "error_file": "./logs/err.log",
    "out_file": "./logs/out.log",
    "merge_logs": true,
    "log_date_format": "YYYY/MM/DD HH:mm:ss",
    "min_uptime": "60s",
    "max_restarts": 17,
    "autorestart": true,
    "restart_delay": "60"
  }
}
